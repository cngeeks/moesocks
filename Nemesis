import Prelude ()
import System.Nemesis.Env
import Air.Env
import Control.Monad

main = run nemesis

nemesis = do

  desc "halive"
  task "halive" - do
    sh "halive src/Main.hs src"


  desc "test"
  task "test" - do
    forever - do
      -- sh "telnet localhost 1090;exit 0"*/
      sh "curl -x socks5://localhost:1090 jinjing.cc;exit 0"
      sleep 1

